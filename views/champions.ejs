<!doctype html>

<html lang="en">
   <head>
     <meta charset="utf-8">

     <title>League Wiki</title>
     <meta name="description" content="The HTML5 Herald">
     <meta name="author" content="SitePoint">

     <link href="https://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.3/css/foundation.min.css" rel="stylesheet" type="text/css" />
     <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
     <link rel="stylesheet" href="/css/styles.css">

     <!--[if lt IE 9]>
     <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
     <![endif]-->
   </head>


   <body>
      <header>
            <h1>League of Legends Wikipedia</h1>
            <a href="/">Home</a>
            <a href="/champions">Champions</a>
      </header>

      <div>
         <div id="buttons"></div>
         <% for(var i=0; i<data.length; i++) { %>
            <% tags = ''; %>
            <% for(var j=0; j<data[i].tags.length; j++) { %>
               <% tags += data[i].tags[j];  %>
               <% if (j == 0 && data[i].tags.length != 1){
                tags += ','; }  %>
            <% } %>
            <div class="cards darken" data-tags="<%= tags %>">
               <a href=<%= "/champions/" + data[i].id %> ><img class="" src=<%= "http://ddragon.leagueoflegends.com/cdn/img/champion/loading/" + data[i].key + "_0.jpg" %>
                  alt=<%= data[i].name %>  /></a>
               <span class="hidden"><%= data[i].name %></span>
            </div>
         <% } %>
      </div>

      <aside id="left">
      </aside>

      <footer>

      </footer>

      <script src="/js/main.js" type="text/javascript"></script>
   </body>
</html>
